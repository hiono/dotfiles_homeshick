[core]
	editor = emacs
	pager =
	autocrlf = input
[include]
	path = .gitconfig.local
[alias]
	co = checkout
	br = branch
	st = status
	clog = log --graph --pretty=\"format:%C(yellow)%h%Cblue%d%Creset %s %C(black bold)%an, %ar%Creset\"
[credential]
	helper = cache --timeout=1800
[push]
	default = matching
	followTags = true
[color]
	ui = auto
[http]
	sslverify = false
[diff]
	compactionHeuristic = true
